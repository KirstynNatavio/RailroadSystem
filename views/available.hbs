<div class="container-contact100">
		<div class="wrap-contact100">
			<form class="contact100-form validate-form" action='/availableConfirm' method='POST'>
				<span class="contact100-form-title">
					Available Trains
				</span>
				{{#if empty}}
					<form class="contact100-form validate-form" action='/dashboard' method='GET'>
						<span class="label-input100">No trains are available</span>
						<div class="container-contact100-form-btn">
							<button class="contact100-form-btn">
								<span>
									Make changes to reservation form
									<i class="fa fa-long-arrow-left m-l-7" aria-hidden="true"></i>
								</span>
							</button>
						</div>
					</form>
				{{else}}
							
					<fieldset class="radiogroup"> 
						<div class="w-full">
						<div class="wrap-contact100-form-radio">

						<div class="contact100-form-radio m-t-15">
						    {{#eachInMap train_arrivals}}
		    						<input class="input-radio100" value="{{key}}" type="radio" id="{{key}}" name="available" checked="checked" />
		    						<label class="label-radio100" for="{{key}}">Train {{key}}, Arrival Time: {{value}}</label>
		    				{{/eachInMap}}
						</div>
				
				

						</div>
					    </div>

					</fieldset>


	

					<div class="container-contact100-form-btn">
						<button class="contact100-form-btn">
							<span>
								Submit
								<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
							</span>
						</button>
					</div>
				{{/if}}
				
			


		      

			</form>
		</div>
	</div>



