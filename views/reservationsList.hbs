{{#if valid}
<div class="container-contact100-login">
		<div class="wrap-contact100">
			<form class="contact100-form validate-form" action='/' method='GET'>
				<span class="contact100-form-title">
					Reservation List
				</span>

				<p class="reservationInfo">RESERVATION ID: {{reservationObj.RESERVATION_ID}}<br>EMAIL: {{passengerObj.EMAIL}}</p>
				<table>
					<tr>
						<th>TRAIN NO</th>
						<th>ORIGIN</th>
						<th>DESTINATION</th>
						<th>TRIP DATE</th>
						<th>TRIP TIME</th>
					</tr>
					<tr>
						<th>{{tripObj.TRAIN_ID}}</th>
						<th>{{originObj.CITY}}, {{originObj.STATE}}</th>
						<th>{{destinationObj.CITY}}, {{destinationObj.STATE}}</th>
						<th>{{tripObj.TRIP_DATE}}</th>
						<th>{{tripObj.TRIP_TIME}}</th>
					</tr>
				</table>
				
				
				<!-- DELETE RESERVATION FORM -->
			
			

				<div class="container-contact100-form-btn">
					<button class="contact100-form-btn">
						<span>
							Main Page
							<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
						</span>
					</button>
				</div>
				


			</form>
			
			<div class="wrap-contact100">
				<form class="contact100-form validate-form" action='/deleteConfirm' method="POST">
					<div class="wrap-input100 input100-select bg1">
						<span class="label-input100">Cancel your reservation</span>

						<div class="container-contact100-form-btn">
						<button class="contact100-form-btn">
							<span>
								Cancel
								<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
							</span>
						</button>
					</div>
				</form>
			</div>

		</div>
	</div>
</div>
{{else}}
<div class="container-contact100-login">
		<div class="wrap-contact100">
			<form class="contact100-form validate-form" action='/' method='GET'>
				<span class="contact100-form-title">
					No record was found for the given information. Please try again. 
				</span>

			
			

				<div class="container-contact100-form-btn">
					<button class="contact100-form-btn">
						<span>
							Main Page
							<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
						</span>
					</button>
				</div>
				


			</form>
			
			<div class="wrap-contact100">
				<form class="contact100-form validate-form" action='/deleteConfirm' method="POST">
					<div class="wrap-input100 input100-select bg1">
						<span class="label-input100">Cancel your reservation</span>

						<div class="container-contact100-form-btn">
						<button class="contact100-form-btn">
							<span>
								Cancel
								<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
							</span>
						</button>
					</div>
				</form>
			</div>

		</div>
	</div>
</div>

{{/if}}


